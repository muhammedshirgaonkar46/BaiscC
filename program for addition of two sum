

#include <stdio.h>
main()
{
    int a[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int b[3][3]={{11,21,31},{41,51,61},{71,81,19}};
    int c[3][3];
    int i,j;
    
    for(i=1;i<=3;i++)
    {
        for(j=0;j<=3;j++)
        {
           c[i][j]=a[i][j]+b[i][j];
            
        }
    }    
    printf("sum is \n");
     for(i=1;i<=3;i++)
    {
        for(j=0;j<=3;j++)
        {
            printf("%d ",c[i][j]);
            
        }
        printf("\n");
    } 
    
    
        
}
   
