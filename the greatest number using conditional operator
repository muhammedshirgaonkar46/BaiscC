#include <stdio.h>
int main()
{ 
    int a,b,c;
    printf("enter first no.a=");
    scanf("%d",&a);
    printf("enter second no.b=");
    scanf("%d",&b);
    printf("enter third no.c=");
    scanf("%d",&c);
    if(a>b&&a>c)
    {
    printf("a is greatest number");
    }
    else
    {
        if(b>c)
        {
            printf("b is greatest number");
        }
        else
        {
            printf("c is greatest number");
        }
    }
}
