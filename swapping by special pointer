#include <stdio.h>
void swap (int *a,int *b)
{
    int temp;
    temp=*a;
    *a=*b;
    *b=temp;
}

main()
{
    int a,b;
    printf("enter the value of a,b=");
    scanf("%d%d",&a, &b);
    printf("\n***BEFORE SWAPPING***\n");
    printf("a=%d,b=%d",a,b);
    swap(&a,&b);
    printf("\n***AFTER SWAPPING***\n");
    printf("a=%d b=%d",a,b);
}
