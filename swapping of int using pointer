#include <stdio.h>

main()
{
    int a;
    a=10;
    printf("value of a=%d",a);
    printf("\naddress of a=%d",&a);
    a=20;
    printf("\n------------\n");
    printf("value of a=%d",a);
    printf("\naddress of a=%d",&a);
    int *spy;
    spy=&a;
    *spy=50;
    printf("\n------------\n");
    printf("value of spy=%d",spy);
    printf("\naddress of spy=%d",&spy);
    printf("\n---------\n");
    printf("value of a=%d",a);
    
    
}
